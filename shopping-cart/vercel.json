{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "dest": "/static/$1"
    },
    {
      "src": "/(favicon\\.ico|favicon\\.png|favicon_frozen\\.png|favicon_frozen_v2\\.png|logo\\.png|.*\\.(css|js|svg|jpg|jpeg|gif|webp))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}



